{
  "name": "@magijs/openapi",
  "version": "0.0.1-alpha.5",
  "description": "OpenAPI",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "node lib/index.js",
    "build": "yarn run build-cjs && yarn run build-esm",
    "build-cjs": "rimraf ./lib && tsc",
    "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015",
    "watch": "rimraf ./lib && tsc --watch"
  },
  "files": [
    "lib",
    "esm",
    "templates"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "glob": "^7.1.6",
    "lodash": "^4.17.20",
    "memoizee": "^0.4.15",
    "mockjs": "^1.1.0",
    "node-fetch": "^2.6.1",
    "nunjucks": "^3.2.2",
    "openapi3-ts": "^2.0.1",
    "reserved-words": "^0.1.2",
    "swagger2openapi": "^7.0.4"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "np": "^7.2.0"
  },
  "gitHead": "c02d77a5ce67159b765955688f7e110bd8f225de"
}
